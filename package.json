    "main": "dist/app.js",
    "scripts": {
        "watch": "npx tsc && concurrently \"npx tsc -w\" \"nodemon dist/app.js\"",
        "build": "npx tsc && npx tscp",
        "start": "node dist/app.js",
        "lint": "eslint . --ext .ts",
        "lint:error": "eslint . --ext .ts --quiet",
        "lint:fix": "eslint . --ext .ts --fix",
        "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "test": "jest --verbose"
    },
    "jest": {
        "preset": "ts-jest",
        "setupFilesAfterEnv": [
            "./src/test/testSetup.ts"
        ]
    },
    "type": "commonjs",
    "author": "",
    "dependencies": {
        "@types/multer": "^1.4.7",
        "body-parser": "^1.20.2",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-validator": "^6.15.0",
        "jest-extended": "^3.2.4",
        "multer": "^1.4.5-lts.1",
        "node-mocks-http": "^1.12.2",
        "pug": "^3.0.2",
        "ts-node": "^10.9.1",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@jest-mock/express": "^2.0.1",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.0",
        "@types/pug": "^2.0.6",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "concurrently": "^7.6.0",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.7.0",
        "jest": "^29.5.0",
        "nodemon": "^2.0.21",
        "prettier": "^2.8.4",
        "supertest": "^6.3.3",
        "ts-jest": "^29.0.5",
        "typescript": "^4.9.5",
        "typescript-cp": "^0.1.7"
    }
